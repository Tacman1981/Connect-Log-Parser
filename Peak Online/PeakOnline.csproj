<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>peak.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="peak.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="peak.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="peak.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
